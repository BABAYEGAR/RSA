<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>@ViewBag.Title</title>
    <!-- Bootstrap Css -->
    <link href="~/Landing/assets/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- Normalize Css -->
    <link href="~/Landing/assets/Normalize/normalize.css" rel="stylesheet">
    <!--Font Awesome Css-->
    <link href="~/Landing/assets/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!--Linear icon Css-->
    <link href="~/Landing/assets/linearicons/css/icon-font.min.css" rel="stylesheet">
    <!--Animate Css-->
    <link href="~/Landing/assets/animate/animate.css" rel="stylesheet">
    <!--Animate headline css-->
    <link href="~/Landing/assets/cd-intro/headline.css" rel="stylesheet">
    <!--Owl carousel css-->
    <link href="~/Landing/assets/owlcarousel/css/owl.carousel.css" rel="stylesheet">
    <link href="~/Landing/assets/owlcarousel/css/owl.theme.css" rel="stylesheet">
    <!--Portfolio Css-->
    <link href="~/Landing/assets/css/ionicons.min.css" rel="stylesheet">
    <link href="~/Landing/assets/css/magnific-popup.css" rel="stylesheet">
    <!--Slicknav Css-->
    <link href="~/Landing/assets/slicknav/slicknav.css" rel="stylesheet">
    <!--Custum Css-->
    <link href="~/Landing/css/style.css" rel="stylesheet">
    <!--Responsive Css-->
    <link href="~/Landing/css/responsive.css" rel="stylesheet">
    <!--Modernizr Js-->
    <script src="~/Landing/js/vendor/modernizr-3.5.0.min.js"></script>
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
<!-- Pre Loader -->
<div id="loader"></div>
<!--Header-->
@{
    await Html.RenderPartialAsync("Partials/LandingHeader");
}
@RenderBody()
@{
    await Html.RenderPartialAsync("Partials/LandingFooter");
}
<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="~/Landing/assets/jquery/jquery-3.2.1.min.js"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="~/Landing/assets/bootstrap/js/bootstrap.min.js"></script>
<script src="~/Landing/assets/easing/jquery.easing.min.js"></script>
<script src="~/Landing/assets/isotope/jquery.isotope.js"></script>
<script src="~/Landing/assets/jquery/imagesloaded.pkgd.min.js"></script>
<script src="~/Landing/assets/cd-intro/main.js"></script>
<script src="~/Landing/assets/wow/wow.min.js"></script>
<script src="~/Landing/assets/slicknav/jquery.slicknav.js"></script>
<script src="~/Landing/assets/owlcarousel/js/owl.carousel.min.js"></script>
<script src="~/Landing/assets/jquery/jquery.magnific-popup.min.js"></script>
<script src="~/Landing/assets/number-animation/jquery.animateNumber.min.js"></script>
<!-- Contact Form Script -->
<script src="~/Landing/assets/contact-form/js/validator.min.js"></script>
<script src="~/Landing/assets/contact-form/js/form-scripts.js"></script>
<script src="~/Landing/assets/jquery/plugins.js"></script>
<script src="~/Landing/js/custom.js"></script>
</body>
</html>